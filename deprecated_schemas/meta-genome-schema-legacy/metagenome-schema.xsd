            <xsd:element form="unqualified" minOccurs="0" maxOccurs="1" name="compressive-poissons-ratio-iso" type="compressive-poissons-ratio-iso-type">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Compressive poissons ratio data</label>
