                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="0" maxOccurs="1" name="compressive-modulus-iso" type="compressive-modulus-iso-type">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Compressive modulus data</label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="0" maxOccurs="1" name="tensile-poissons-ratio-iso" type="tensile-poissons-ratio-iso-type">
                <xsd:annotation>
