<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<xsd:schema attributeFormDefault="unqualified" elementFormDefault="unqualified"
    xmlns:ds="http://datacite.org/schema/kernel-4" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    

    <xsd:complexType name="attribute-base"> </xsd:complexType>

<!--======================================================================-->
<!--        Creating fields for general metamaterial information          --> 
<!--======================================================================-->
    <xsd:complexType name="general-metamaterial-info">
        
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="unbounded" name="family">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>metamaterial family</label>
                        <documentation>This block should pass meta data to the parser</documentation>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="Foam"/>
                        <xsd:enumeration value="Textile"/>
                        <xsd:enumeration value="Fibre reinforced composite"/>
                        <xsd:enumeration value="Idealised lattice"/>
                        <xsd:enumeration value="Honeycomb"/>
                        <xsd:enumeration value="Kirigami"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>

            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="unusual_property">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Unusual material property</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="negative Poissons ratio"/>
                        <xsd:enumeration value="negative stiffness"/>
                        <xsd:enumeration value="negative compressability"/>
                        <xsd:enumeration value="force/torque coupling"/>
                        <xsd:enumeration value="negative moisture expansion"/>
                        <xsd:enumeration value="negative thermal expansion"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>

            <xsd:element form="unqualified" name="description" minOccurs="0" maxOccurs="1" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <module>module-text-area</module>
                        <label>Metamaterial description</label>
                        <placeholder>Please provide a short description of your metamaterial here.</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>

            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="num_base_mats" type="xsd:integer">
                <xsd:annotation>
                        <xsd:appinfo>
                            <label>number of base material components</label>
                            <placeholder>E.g. 1, 2, 3...</placeholder>
                        </xsd:appinfo>
                    </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

<!--======================================================================-->
<!--                Creating type for testing conditions                  --> 
<!--======================================================================-->
    <xsd:complexType name="test-conditions">
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="test-temperature" type="xsd:float">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Test temperature</label>
                        <placeholder>E.g. 273.15</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="testing-temperature-units">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Testing temperature units</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="&#730;C"/>
                        <xsd:enumeration value="&#730;K"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="relative-humidity" type="xsd:float">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Relative humidity (%)</label>
                        <placeholder>E.g. 60</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="loading-rate" type="xsd:float">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Test loading rate</label>
                        <placeholder>E.g. 0.1</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="loading-rate-unit">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>loading rate units</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="cm/s"/>
                        <xsd:enumeration value="mm/s"/>
                        <xsd:enumeration value="%/s"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

<!--======================================================================-->
<!--             Creating type for capturing xls - stress strain          --> 
<!--======================================================================-->
    
    <xsd:complexType xmlns:ns0="http://mdcs.ns" name="Table" ns0:_mod_mdcs_="/diffusion/upload-excel">
        <xsd:annotation>
            <xsd:appinfo>
                <module>module-excel-uploader</module>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="headers" type="Headers"/>
            <xsd:element name="rows" type="Rows"/>
        </xsd:sequence>
        <xsd:attribute name="name" type="xsd:string" use="optional"/>
    </xsd:complexType>

    <xsd:complexType name="Headers">
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" name="column" type="Column"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="Rows">
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" name="row" type="Row"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="Row">
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" name="column" type="Column"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:string" use="required"/>
    </xsd:complexType>
    <xsd:complexType name="Column">
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:attribute name="id" type="xsd:string" use="required"/>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="xls-upload-stress-strain">
        <xsd:sequence>
            <xsd:element name="fileExtension" type="xsd:string"/>
            <xsd:element name="xls-upload-stress-strain-table" type="Table">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>
                        Please upload Excel 97-2003 Workbook .xls file for stress strain data.
                        .xls file MUST contain strain data (%) in column A and stress data in column B only WITH row 1 blank.
                        </label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

<!--======================================================================-->
<!--        Creating type for capturing xls - ax_strain-t_strain          --> 
<!--======================================================================-->
    <xsd:complexType name="xls-upload-ax_strain-t_strain">
        <xsd:sequence>
            <xsd:element name="fileExtension" type="xsd:string"/>
            <xsd:element name="xls-upload-ax_strain-t_strain-table" type="Table">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>
                            Please upload Excel 97-2003 Workbook .xls file for axial strain vs transverse strain data.
                            .xls file MUST contain axial strain data (%) in column A and transverse strain data (%) data in column B only WITH row 1 blank.
                        </label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

<!--======================================================================-->
<!--             Creating type for capturing xls - stress relaxation      --> 
<!--======================================================================-->
    <xsd:complexType name="xls-upload-strain-stress-relax">
        <xsd:sequence>
            <xsd:element name="fileExtension" type="xsd:string"/>
            <xsd:element name="xls-upload-strain-stress-relax-table" type="Table">
                <xsd:annotation>
                    <xsd:appinfo>
                        <module>module-excel-uploader</module>
                    </xsd:appinfo>
                    <xsd:appinfo>
                        <label>
                            Please upload Excel 97-2003 Workbook .xls file for axial strain vs stress relaxation data.
                            .xls file MUST contain axial strain data (%) in column A and stress relaxation data in column B only WITH row 1 blank.
                        </label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>


<!--======================================================================-->
<!--                    Creating type for loading axis                    --> 
<!--======================================================================-->
    <xsd:complexType name="loading-ax-capture">
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="loading-ax">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>loading axis</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="x"/>
                        <xsd:enumeration value="y"/>
                        <xsd:enumeration value="z"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

<!--======================================================================-->
<!--                    Creating type for transverse ax                    --> 
<!--======================================================================-->
    <xsd:complexType name="transverse-ax-capture">
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="transverse-ax">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>transverse axis</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="x"/>
                        <xsd:enumeration value="y"/>
                        <xsd:enumeration value="z"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

<!--======================================================================-->
<!--                    Creating types for stress-strain                  --> 
<!--======================================================================-->

    <xsd:complexType name="stress-strain-points">
        <xsd:sequence>
            <xsd:annotation>
                <xsd:appinfo>
                    <label>Stress strain data</label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="stress-strain-loading-ax" type="loading-ax-capture"/>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="stress-strain-unit">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Stress units</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="KPa"/>
                        <xsd:enumeration value="MPa"/>
                        <xsd:enumeration value="GPa"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Choose to upload .xls (excel 97-2003) file or manually input data points</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:element form="unqualified" minOccurs="1" maxOccurs="unbounded" type="stress-strain-point" name="stress-strain-input"/>
                <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" type="xls-upload-stress-strain" name="stress-strain-xls">
                    <xsd:annotation>
                    <xsd:appinfo>
                        <label>
                            upload Excel workbook/spreadsheet
                        </label>
                    </xsd:appinfo>
                </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="stress-strain-point">
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="stress" type="xsd:float"/>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="strain" type="xsd:float"/>
        </xsd:sequence>
    </xsd:complexType>


<!--======================================================================-->
<!--              Creating types for transverse strain-strain             --> 
<!--======================================================================-->

<xsd:complexType name="trans-axial-strain-points">

        <xsd:sequence>
            <xsd:annotation>
                <xsd:appinfo>
                    <label>Transverse vs. axial strain data</label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="trans-axial-strain-loading-ax" type="loading-ax-capture">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Axial loading axis</label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="trans-axial-strain-trans-ax" type="transverse-ax-capture">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Transverse loading axis</label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:choice minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Choose to upload .xls (excel 97-2003) file or manually input data points</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:element form="unqualified" minOccurs="1" maxOccurs="unbounded" type="trans-axial-strain-point" name="trans-axial-strain-input"/>
                <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" type="xls-upload-ax_strain-t_strain" name="trans-axial-strain-xls">
                    <xsd:annotation>
                    <xsd:appinfo>
                        <label>
                            upload Excel workbook/spreadsheet
                        </label>
                    </xsd:appinfo>
                </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="trans-axial-strain-point">
        <xsd:annotation>
            <xsd:appinfo>
                <label>transverse strain data</label>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="engineering-strain" type="xsd:float"/>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="transverse-strain" type="xsd:float"/>
        </xsd:sequence>
    </xsd:complexType>

<!--======================================================================-->
<!--              Creating types for stress relaxation                    --> 
<!--======================================================================-->

<xsd:complexType name="stress-relax-points">
        <xsd:sequence>
            <xsd:annotation>
                <xsd:appinfo>
                    <label>Transverse vs axial strain data</label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="stress-relax-loading-ax" type="loading-ax-capture"/>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="stress-relax-strain-unit">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>stress relaxation units</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="KPa/s"/>
                        <xsd:enumeration value="MPa/s"/>
                        <xsd:enumeration value="GPa/s"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Choose to upload .xls (excel 97-2003) file or manually input data points</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:element form="unqualified" minOccurs="1" maxOccurs="unbounded" type="stress-relax-point" name="stress-relax-input"/>
                <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" type="xls-upload-ax_strain-t_strain" name="stress-relax-xls">
                    <xsd:annotation>
                    <xsd:appinfo>
                        <label>
                            upload Excel workbook/spreadsheet
                        </label>
                    </xsd:appinfo>
                </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="stress-relax-point">
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="time" type="xsd:float">
            <xsd:annotation>
                    <xsd:appinfo>
                        <label>Time</label>
                        <placeholder></placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="stress-relaxation" type="xsd:float">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Stress relaxation</label>
                        <placeholder></placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>


<!--======================================================================-->
<!-- Setting up directionality sensitive elastic properties input fields  --> 
<!--======================================================================-->

<!--======================================================================-->
<!--                             ISOTROPIC                                --> 
<!--======================================================================-->
