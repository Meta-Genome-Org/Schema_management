    <xsd:complexType name="parameters">
        <xsd:sequence>
            <xsd:element name="param" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>parameter descriptor</label>
                        <placeholder>e.g. h13, l13</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element> 
            <xsd:element name="param-value" type="xsd:float">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>parameter value</label>
                        <placeholder>e.g. 0.1</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element> 
            <xsd:element name="param-unit" type="xsd:float">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>parameter unit</label>
                        <placeholder>e.g. degrees</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element> 
        </xsd:sequence>
    </xsd:complexType>

