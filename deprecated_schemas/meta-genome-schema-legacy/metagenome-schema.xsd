                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="0" maxOccurs="unbounded" name="metamaterial-trans-axial-strain" type="trans-axial-strain-points">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Metamaterial Transverse vs axial strain data</label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            
            <xsd:element form="unqualified" minOccurs="0" maxOccurs="unbounded" name="metamaterial-stress-relax" type="stress-relax-points">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Metamaterial Stress relaxation data</label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>


    


<!--======================================================================-->
<!--                     reference and submitter details                  -->
<!--======================================================================-->
    <xsd:complexType name="reference">
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="submitter-first-name" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Submitter first name</label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="submitter-last-name" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Submitter last name</label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="submitter-email" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Submitter email address</label>
                        <placeholder>jsmith@place.gov</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>

        </xsd:sequence>
    </xsd:complexType>


    <xsd:complexType name="publication-details">
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="id" type="xsd:anyURI">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Publication DOI</label>
                        <placeholder>E.g. http://dx.doi.org/123/abc</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="unbounded" name="publication-authors" type="authors"/>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="publication-title" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Publication title</label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="publication-year" type="xsd:integer">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Publication year</label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="publication-journal" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Publication journal</label>
                        <placeholder>e.g. Nature</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="publication-issue" type="xsd:integer">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Publication issue</label>
                        <placeholder>e.g. 1</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="publication-page" type="xsd:integer">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Publication page</label>
                        <placeholder>e.g. 1</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="publication-url" type="xsd:anyURI">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Publication url</label>
                        <placeholder>e.g. nature.com/mypublicaton</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="Publication-submitter" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>submitter name</label>
                        <placeholder>Bill Smith</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="Publication-submitter-email" type="emailAddress">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>submitter email</label>
                        <placeholder></placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="emailAddress"> 
        <xsd:restriction base="xsd:string"> 
            <xsd:pattern value="[^@]+@[^\.]+\..+"/> 
        </xsd:restriction> 
    </xsd:simpleType> 

    <xsd:complexType name="authors">
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="author-initials" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Author initial</label>
                        <placeholder>e.g. A., A. B., A. B. C.,</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="author-surname" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Author Surname</label>
                        <placeholder>e.g. Feynman</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>




<!--======================================================================-->
<!--                     CALLING ALL FIELDS AND COMPONENTS                -->
<!--======================================================================-->
    <xsd:complexType name="genome-map">
        
                <xsd:sequence>
                    <xsd:element form="unqualified" minOccurs="0" maxOccurs="1" name="metamaterial-publication-info" type="publication-details">
                        <xsd:annotation>
                            <xsd:appinfo>
                                <label>Reference information</label>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element form="unqualified" minOccurs="0" maxOccurs="1" name="general-info" type="general-metamaterial-info" >
                        <xsd:annotation>
                            <xsd:appinfo>
                                <label>metamaterial general information</label>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    
                    <xsd:element form="unqualified" minOccurs="0" maxOccurs="unbounded" name="base-material-info" type="base-material-properties" >
                        <xsd:annotation>
                            <xsd:appinfo>
                                <label>base material properties</label>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    
                    <xsd:element form="unqualified" minOccurs="0" maxOccurs="unbounded" name="metamaterial-material-info" type="metamaterial-properties" >
                        <xsd:annotation>
                            <xsd:appinfo>
                                <label>metamaterial properties</label>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    
                </xsd:sequence>
            
    </xsd:complexType>
    <xsd:element name="map" type="genome-map"/>

</xsd:schema>

        
                