<!--======================================================================-->
<!--                        BASE MATERIAL PROPERTIES                      -->
<!--======================================================================-->
    <xsd:complexType name="base-material-properties"> 
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="material_name" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Material name</label>
                        <placeholder>provide generic material name</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>

            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="material_class">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Material Classification</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="Metallic"/>
                        <xsd:enumeration value="Ceramic"/>
                        <xsd:enumeration value="Organic"/>
                        <xsd:enumeration value="Inorganic"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            
    
            <xsd:element name="chemical-composition" type="ConstituentsType">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Chemical composition</label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>

            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="bulk-density" type="xsd:float">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Bulk density</label>
                    </xsd:appinfo>
                </xsd:annotation>

            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="bulk-density-unit">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Bulk density units</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="g/cm&#179;"/>
                        <xsd:enumeration value="kg/m&#179;"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="0" maxOccurs="1" name="von-mises" type="xsd:float">
                <xsd:annotation>
                        <xsd:appinfo>
                            <label>Von Mises Stress</label>
                        </xsd:appinfo>
                    </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="0" maxOccurs="1" name="von-mises-unit">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Von Mises Stress Unit</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="KPa"/>
                        <xsd:enumeration value="MPa"/>
                        <xsd:enumeration value="GPa"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            

            <xsd:element form="unqualified" name="description" minOccurs="0" maxOccurs="1" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <module>module-text-area</module>
                        <label>base material description</label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>

            <xsd:choice minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                        <xsd:appinfo>
                            <label>Directional Sensitivity</label>
                        </xsd:appinfo>
                    </xsd:annotation>
                <xsd:element form="unqualified" minOccurs="0" maxOccurs="0" name="Select">
                    <xsd:annotation>
                            <xsd:appinfo>
                                <label>Please select sensitivity</label>
                            </xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="isotropic-choice" type="elastic-properties-isotropic">
                    <xsd:annotation>
                        <xsd:appinfo>
                            <label>Isotropic data</label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="transversely-isotropic-choice" type="elastic-properties-trans">
                    <xsd:annotation>
                        <xsd:appinfo>
                            <label>Transversely Isotropic data</label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="orthotropic-choice" type="elastic-properties-ortho">
                    <xsd:annotation>
                        <xsd:appinfo>
                            <label>Orthotropic data</label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
            <xsd:element form="unqualified" minOccurs="0" maxOccurs="unbounded" name="base-stress-strain" type="stress-strain-points">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>stress strain data</label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="0" maxOccurs="unbounded" name="base-trans-axial-strain" type="trans-axial-strain-points">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Axial vs transverse strain data</label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            
            <xsd:element form="unqualified" minOccurs="0" maxOccurs="unbounded" name="base-stress-relax" type="stress-relax-points">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Stress relaxation data</label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>

        </xsd:sequence>
    </xsd:complexType>
            

